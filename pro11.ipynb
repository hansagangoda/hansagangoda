   "source": [
    "#pro 11. prime or not prime number print \n",
    "num=int(input(\"Enter the number:\"))\n",
    "if num>1:\n",
    "    for i in range(2,num):\n",
    "        if(num%i)==0:\n",
    "          print(num,\"is not prime\")\n",
    "          break\n",
    "    else:\n",
    "          print(num,\"is prime number\")"
   ]
  }
